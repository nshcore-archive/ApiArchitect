{
    "name": "apiarchitect/apiarchitect",
    "description": "ApiArchitect boiler plate framework, raqpidly develop new apis on the lumen framework.",
    "keywords": ["framework", "laravel", "lumen", "api", "boilerplate", "apiarchitect"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "apiarchitect/compass": "dev-master",
        "apiarchitect/log": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "appzcoder/lumen-routes-list": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
